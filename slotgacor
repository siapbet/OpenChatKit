<script>
  const container = document.querySelector('.fireworkss')

const fireworks = new Fireworks({
    target: container,
    hue: 120,
    startDelay: 5,
    minDelay: 20,
    maxDelay: 1,
    speed: 1,
    acceleration: 1.04,
    friction: 0.98,
    gravity: 1,
    particles: 99,
    trace: 1,
    explosion: 5,
    boundaries: {
        top: 50,
        bottom: container.clientHeight,
        left: 50,
        right: container.clientWidth
    },
})

// start fireworks
fireworks.start()
</script>
</center>
